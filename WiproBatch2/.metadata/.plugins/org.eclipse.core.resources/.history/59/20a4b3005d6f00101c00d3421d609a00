package Day12;

public class threading extends Thread{
	
	static int c = 0;
	
	public void run() {
		System.out.println("This method is running in thread");
	}
	
	public void inc() {
		c = c+1;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Thread t1 = new threading();
		c++;
		c++;
		c++;		
		System.out.println(c);
	}

}
