package labsessions.Day12;

class evenOdd extends Thread{
	int i;
	evenOdd(int i){
		this.i = i;
	}
	public void run() {
		for(int j=0;j<5;j++) {
			System.out.print(i+",");
			i+=2;
		}
	}
}
public class evenoddThread {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Thread t1 = new evenOdd(1);
		Thread t2 = new evenOdd(2);
		t1.start();
		t2.start();
	}

}
