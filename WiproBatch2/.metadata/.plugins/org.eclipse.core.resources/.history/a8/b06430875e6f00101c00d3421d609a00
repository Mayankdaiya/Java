package Day12;

public class threading extends Thread{
	
	static int c = 0;
	
	public void run() {
		c++;
		System.out.println(c);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Thread t1 = new threading();
		Thread t2 = new threading();
		Thread t3 = new threading();
		t1.start();
		t2.start();
		t3.start();
		
//		System.out.println(c);
	}

}
