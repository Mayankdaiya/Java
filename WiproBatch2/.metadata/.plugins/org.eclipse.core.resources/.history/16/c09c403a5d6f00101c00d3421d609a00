package Day12;

public class threading extends Thread{
	
	int c = 0;
	
	public void run() {
		c++;
		System.out.println(c);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Thread t1 = new threading();
		t1.start();
		t1.start();
		t1.start();
//		System.out.println(c);
	}

}
